<packages>
	<httpd>
		<alternative>
			<apache>apache2</apache>
			<apache>apache2-worker</apache>
			<apache>apache2-mod_fcgid</apache>
			<apache>apache2-mod_fastcgi</apache>
			<apache>apache2-mod_cband</apache>
			<apache>apache2-mod_proxy_html</apache>
		</alternative>
	</httpd>
	<po>
		<alternative>
			<courier>courier-imap</courier>
			<courier>courier-authlib-userdb</courier>
			<courier>-courier-authlib-ldap</courier><!-- authlib falls back to all installed modules by default -->
			<courier>-courier-authlib-mysql</courier>
			<courier>-courier-authlib-pgsql</courier>
			<courier>-courier-authlib-pipe</courier>
			<courier>fam-server</courier><!-- courier needs FAM, ... -->
			<courier>rpcbind</courier><!-- ... which relies on rcpbind -->
			<dovecot>dovecot20</dovecot>
			<dovecot>dovecot-backend-mysql</dovecot>
		</alternative>
	</po>
	<mta>
		<alternative>
			<postfix>postfix</postfix>
		</alternative>
	</mta>
	<ftpd>
		<alternative>
			<proftpd>proftpd</proftpd>
			<proftpd>proftpd-mysql</proftpd>
		</alternative>
	</ftpd>
	<named>
		<alternative>
			<bind>bind</bind>
			<bind>bind-chrootenv</bind>
		</alternative>
	</named>
	<sql>
		<mysql>mysql-community-server</mysql>
		<mysql>mysql-community-server-client</mysql>
	</sql>
	<php>
		<php5>php5-pear</php5>
		<php5>php5</php5>
		<php5>php5-ftp</php5>
		<php5>php5-gd</php5>
		<php5>php5-gettext</php5>
		<php5>&apos;php5-intl&gt;=5.3.8&apos;</php5><!-- on openSuSE 11.4, php5-intl-5.3.5 is broken -->
		<php5>php5-mbstring</php5>
		<php5>php5-mcrypt</php5>
		<php5>php5-mysql</php5>
		<php5>php5-sockets</php5>
	</php>
	<perl>
		perl
		<library>perl-BerkeleyDB</library>
		<library>perl-Crypt-Blowfish</library>
		<library>perl-Crypt-CBC</library>
		<library>perl-Crypt-PasswdMD5</library>
		<library>perl-Data-Validate-Domain</library>
		<library>perl-Data-Validate-IP</library>
		<library>perl-Date-Calc</library>
		<library>perl-Date-Manip</library>
		<library>perl-DateTime</library>
		<library>perl-DBD-mysql</library>
		<library>perl-DBI</library>
		<library>perl-Email-Valid</library>
		<library>perl-File-Copy-Recursive</library>
		<library>perl-File-MimeInfo</library>
		<library>perl-IO-Socket-INET6</library>
		<library>perl-IO-stringy</library>
		<library>perl-Mail-Sendmail</library>
		<library>perl-MailTools</library>
		<library>perl-MIME-tools</library>
		<library>perl-Net-DNS</library>
		<library>perl-Net-LibIDN</library>
		<library>perl-Net-Netmask</library>
		<library>perl-SMTP-Server</library>
		<library>perl-SNMP</library>
		<library>perl-Term-ReadKey</library>
		<library>perl-Term-ReadPassword</library>
		<library>perl-HTML-Parser</library>
		<library>perl-TimeDate</library>
		<library>perl-XML-Parser</library>
		<library>perl-XML-Simple</library>
		<library>perl-Email-Simple</library>
	</perl>
